<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="planner.car.dav.com.mycarplanner"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="planner.car.dav.com.mycarplanner.Acceuil"
            android:label="@string/title_activity_acceuil" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="planner.car.dav.com.mycarplanner.Car"
            android:label="@string/title_activity_car" >
        </activity>
        <activity
            android:name="planner.car.dav.com.mycarplanner.AddCar"
            android:label="@string/title_activity_car"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="planner.car.dav.com.mycarplanner.AndroidDatabaseManager"
            android:label="@string/title_activity_car" >
        </activity>
        <activity
            android:name="planner.car.dav.com.mycarplanner.Spending"
            android:label="@string/title_activity_spending"
            android:windowSoftInputMode="stateHidden" >
        </activity>
    </application>

</manifest>